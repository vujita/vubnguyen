{
  "name": "@vujita/db",
  "version": "0.0.3-alpha.0",
  "private": true,
  "license": "MIT",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "scripts": {
    "clean": "rm -rf node_modules",
    "db:drop": "pnpm with-env drizzle-kit drop",
    "db:generate": "pnpm with-env drizzle-kit generate:pg",
    "db:migrate": "pnpm with-env tsx src/migrate.ts",
    "db:push": "pnpm with-env drizzle-kit push:pg",
    "dev": "pnpm with-env drizzle-kit studio --port 5556",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@auth/core": "0.19.1",
    "drizzle-kit": "0.21.1",
    "drizzle-orm": "0.29.3",
    "postgres": "3.4.3"
  },
  "devDependencies": {
    "dotenv-cli": "7.2.1",
    "pg": "8.11.3",
    "tsx": "4.7.0",
    "typescript": "5.4.5"
  }
}
