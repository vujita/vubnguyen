{
  "name": "@vujita/db",
  "version": "0.0.3-alpha.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "scripts": {
    "clean:modules": "(.*)",
    "db:drop": "pnpm with-env drizzle-kit drop",
    "db:generate": "pnpm with-env drizzle-kit generate",
    "db:migrate": "pnpm with-env drizzle-kit migrate",
    "db:push": "pnpm with-env drizzle-kit push",
    "dev": "pnpm with-env drizzle-kit studio --port 5556",
    "with-env": "dotenv -e ../../.env --"
  },
  "eslintConfig": {
    "extends": [
      "eslint-config-vujita/base"
    ],
    "rules": {
      "no-relative-import-paths/no-relative-import-paths": "off"
    },
    "root": true
  },
  "dependencies": {
    "@auth/core": "0.39.0",
    "drizzle-kit": "0.22.7",
    "drizzle-orm": "0.44.3",
    "postgres": "3.4.4",
    "zod": "3.22.3"
  },
  "devDependencies": {
    "dotenv-cli": "7.4.2",
    "eslint-config-vujita": "0.0.6-alpha.7",
    "pg": "8.15.6",
    "tsx": "4.16.0",
    "typescript": "5.8.3"
  }
}
