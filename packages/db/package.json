{
  "name": "@vujita/db",
  "private": true,
  "license": "MIT",
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "scripts": {
    "clean:modules": "(.*)",
    "db:drop": "pnpm with-env drizzle-kit drop",
    "db:generate": "pnpm with-env drizzle-kit generate",
    "db:migrate": "pnpm with-env drizzle-kit migrate",
    "db:push": "pnpm with-env drizzle-kit push",
    "dev": "pnpm with-env drizzle-kit studio --port 5556",
    "with-env": "dotenv -e ../../.env --"
  },
  "eslintConfig": {
    "extends": [
      "eslint-config-vujita/base"
    ],
    "rules": {
      "no-relative-import-paths/no-relative-import-paths": "off"
    },
    "root": true
  },
  "dependencies": {
    "@auth/core": "catalog:",
    "drizzle-kit": "catalog:",
    "drizzle-orm": "catalog:",
    "postgres": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "dotenv-cli": "catalog:",
    "eslint-config-vujita": "workspace:*",
    "pg": "catalog:",
    "tsx": "catalog:",
    "typescript": "catalog:"
  }
}
