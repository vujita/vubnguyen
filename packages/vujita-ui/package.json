{
  "name": "vujita-ui",
  "version": "0.0.6-alpha.7",
  "license": "MIT",
  "homepage": "https://github.com/vujita/vubnguyen#readme",
  "main": "index.js",
  "module": "index.mjs",
  "types": "index.d.ts",
  "bugs": {
    "url": "https://github.com/vujita/vubnguyen/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vujita/vubnguyen"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "postcss.config.cjs",
    "tailwind.config.ts",
    "*.mjs",
    "*.js"
  ],
  "scripts": {
    "build:clean": "rimraf ./*.mjs ./*.js ./*.mts ./*.d.ts ./*.map",
    "build:tsup": "tsup",
    "build": "xrun -s build:clean build:tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf .turbo node_modules",
    "lint": "eslint .",
    "lint:fix": "pnpm lint --fix",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "clsx": "2.0.0",
    "tailwind-merge": "1.14.0"
  },
  "devDependencies": {
    "@xarc/run": "1.1.1",
    "eslint": "8.45.0",
    "eslint-config-vujita": "0.0.6-alpha.7",
    "rimraf": "5.0.1",
    "tailwindcss": "3.3.2",
    "tsup": "7.2.0",
    "typescript": "5.1.6"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "eslint-config-vujita/base"
    ]
  }
}
