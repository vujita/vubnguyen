{
  "name": "vujita-ui",
  "version": "0.0.6-alpha.7",
  "license": "MIT",
  "homepage": "https://github.com/vujita/vubnguyen#readme",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bugs": {
    "url": "https://github.com/vujita/vubnguyen/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vujita/vubnguyen"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build:clean": "rimraf dist storybook-static",
    "build": "xrun -s build:clean build:lib build:storybook",
    "build:lib": "rm -rf dist && tsup",
    "dev": "xrun build:watch storybook",
    "build:watch": "tsup --watch",
    "clean": "rm -rf .turbo node_modules",
    "lint": "eslint .",
    "lint:fix": "pnpm lint --fix",
    "type-check": "tsc --noEmit",
    "storybook": "storybook dev -p 6006",
    "build:storybook": "storybook build"
  },
  "dependencies": {
    "clsx": "2.0.0",
    "tailwind-merge": "1.14.0"
  },
  "devDependencies": {
    "@babel/preset-env": "7.22.10",
    "@babel/preset-react": "7.22.5",
    "@babel/preset-typescript": "7.22.11",
    "@storybook/addon-essentials": "7.3.2",
    "@storybook/addon-interactions": "7.3.2",
    "@storybook/addon-links": "7.3.2",
    "@storybook/addon-onboarding": "1.0.8",
    "@storybook/blocks": "7.3.2",
    "@storybook/react": "7.3.2",
    "@storybook/react-webpack5": "7.3.2",
    "@storybook/testing-library": "0.2.0",
    "@xarc/run": "1.1.1",
    "eslint": "8.45.0",
    "eslint-config-vujita": "0.0.6-alpha.7",
    "eslint-plugin-storybook": "0.6.13",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rimraf": "5.0.1",
    "storybook": "7.3.2",
    "tsup": "7.2.0",
    "typescript": "5.1.6"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "eslint-config-vujita/base",
      "plugin:storybook/recommended"
    ]
  }
}
