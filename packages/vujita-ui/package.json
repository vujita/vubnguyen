{
  "name": "vujita-ui",
  "version": "0.0.6-alpha.7",
  "homepage": "https://github.com/vujita/vubnguyen#readme",
  "bugs": {
    "url": "https://github.com/vujita/vubnguyen/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vujita/vubnguyen"
  },
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./avatar": {
      "import": "./dist/avatar.mjs",
      "require": "./dist/avatar.js"
    },
    "./classnames": {
      "import": "./dist/classnames.mjs",
      "require": "./dist/classnames.js"
    },
    "./skeleton": {
      "import": "./dist/skeleton.mjs",
      "require": "./dist/skeleton.js"
    },
    "./tailwind.preset": "./src/tailwind.preset.ts",
    "./type-helpers": {
      "import": "./dist/type-helpers.mjs",
      "require": "./dist/type-helpers.js"
    },
    "./package.json": "./package.json",
    "./src/index.css": "./src/index.css",
    "./postcss.config.cjs": "./postcss.config.cjs"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "postcss.config.cjs",
    "tailwind.config.ts",
    "dist",
    "src"
  ],
  "scripts": {
    "build": "xrun -s build:clean build:lib",
    "clean:modules": "(.*)",
    "dev": "xrun build:watch",
    "lint": "eslint .",
    "lint:fix": "pnpm lint --fix",
    "prepare": "xrun -s build:clean build:lib",
    "test": "jest",
    "type-check": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "class-variance-authority": "0.7.0",
    "clsx": "2.1.1",
    "lucide-react": "0.378.0",
    "tailwind-merge": "3.4.0",
    "tailwindcss-animate": "1.0.7"
  },
  "devDependencies": {
    "@babel/core": "7.22.17",
    "@babel/preset-env": "7.22.10",
    "@babel/preset-react": "7.22.5",
    "@babel/preset-typescript": "7.24.1",
    "@types/jest": "29.5.14",
    "@types/node": "25.0.3",
    "@xarc/run": "1.1.1",
    "babel-jest": "29.7.0",
    "dotenv": "16.4.4",
    "eslint": "9.26.0",
    "eslint-config-vujita": "workspace:*",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "react": "19.1.0",
    "react-dom": "19.2.3",
    "rimraf": "5.0.7",
    "tailwindcss": "3.3.2",
    "ts-node": "10.9.1",
    "tsdown": "0.15.10",
    "typescript": "5.9.2"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  },
  "publishConfig": {
    "access": "public"
  }
}
