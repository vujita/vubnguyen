{
  "name": "@vujita/vubnguyen",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "xrun vubnguyen/build",
    "clean:modules": "(.*)",
    "dev": "pnpm with-env next dev",
    "dev:preview": "pnpm with-env next start",
    "e2e": "rimraf playwright-report && pnpm with-env playwright test",
    "eslint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "eslint:fix": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "lint": "xrun -s next:lint eslint",
    "lint:fix": "xrun -s next:lint:fix eslint:fix",
    "next:lint": "dotenv -v SKIP_ENV_VALIDATION=1 next lint",
    "next:lint:fix": "dotenv -v SKIP_ENV_VALIDATION=1 next lint --fix",
    "start": "pnpm with-env next start",
    "type-check": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@fortawesome/free-solid-svg-icons": "6.5.2",
    "@fortawesome/react-fontawesome": "0.2.0",
    "@t3-oss/env-nextjs": "0.6.0",
    "@tanstack/react-query": "5.87.1",
    "@tanstack/react-query-devtools": "5.87.1",
    "@tanstack/react-query-next-experimental": "5.87.1",
    "@trpc/client": "11.5.1",
    "@trpc/next": "11.5.1",
    "@trpc/react-query": "11.5.1",
    "@trpc/server": "11.5.1",
    "@vujita/api": "workspace:*",
    "@vujita/auth": "workspace:*",
    "@vujita/db": "workspace:*",
    "framer-motion": "11.3.7",
    "lucide-react": "0.378.0",
    "next": "15.4.7",
    "next-auth": "4.24.12",
    "next-themes": "0.3.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "superjson": "2.2.1",
    "vujita-ui": "workspace:*",
    "zod": "3.22.3"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "15.3.1",
    "@playwright/test": "1.52.0",
    "@types/node": "22.15.3",
    "@types/react": "19.1.2",
    "@types/react-dom": "19.1.3",
    "@xarc/run": "1.1.1",
    "autoprefixer": "10.4.19",
    "dotenv-cli": "7.4.2",
    "eslint": "8.45.0",
    "eslint-config-vujita": "workspace:*",
    "postcss": "8.4.38",
    "rimraf": "5.0.7",
    "tailwindcss": "3.3.2",
    "typescript": "5.9.2"
  }
}
